<svg width="400" height="300" xmlns="http://www.w3.org/2000/svg">
  <rect width="400" height="300" fill="#9b59b6"/>
  
  <!-- Test paper background -->
  <rect x="80" y="60" width="240" height="180" fill="white" rx="10" stroke="#7f8c8d" stroke-width="2"/>
  
  <!-- Header -->
  <text x="200" y="85" text-anchor="middle" fill="#2c3e50" font-family="Arial" font-size="14" font-weight="bold">Multiplication Tables Check</text>
  <line x1="90" y1="95" x2="310" y2="95" stroke="#bdc3c7" stroke-width="1"/>
  
  <!-- Questions -->
  <text x="100" y="115" fill="#2c3e50" font-family="Arial" font-size="12">1. 7 × 8 = ___</text>
  <text x="100" y="135" fill="#2c3e50" font-family="Arial" font-size="12">2. 9 × 6 = ___</text>
  <text x="100" y="155" fill="#2c3e50" font-family="Arial" font-size="12">3. 4 × 12 = ___</text>
  <text x="100" y="175" fill="#2c3e50" font-family="Arial" font-size="12">4. 11 × 5 = ___</text>
  <text x="100" y="195" fill="#2c3e50" font-family="Arial" font-size="12">5. 8 × 9 = ___</text>
  
  <!-- Timer -->
  <circle cx="320" cy="120" r="25" fill="#e74c3c"/>
  <text x="320" y="127" text-anchor="middle" fill="white" font-family="Arial" font-size="14" font-weight="bold">6s</text>
  
  <!-- Current question highlight -->
  <rect x="95" y="105" width="120" height="18" fill="#f39c12" opacity="0.3" rx="2"/>
  
  <!-- Title -->
  <text x="200" y="30" text-anchor="middle" fill="white" font-family="Arial" font-size="18" font-weight="bold">Times Tables Test</text>
  <text x="200" y="50" text-anchor="middle" fill="#e8d5f2" font-family="Arial" font-size="12">Official Format Practice</text>
  
  <!-- Progress -->
  <text x="200" y="270" text-anchor="middle" fill="white" font-family="Arial" font-size="14">Question 1 of 25</text>
</svg> 