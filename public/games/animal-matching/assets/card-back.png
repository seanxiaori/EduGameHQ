<svg width="200" height="200" xmlns="http://www.w3.org/2000/svg">
  <!-- 背景渐变 -->
  <defs>
    <linearGradient id="cardGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#4CAF50;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#2E7D32;stop-opacity:1" />
    </linearGradient>
    <pattern id="pawPattern" x="0" y="0" width="40" height="40" patternUnits="userSpaceOnUse">
      <circle cx="20" cy="20" r="3" fill="#FFFFFF" opacity="0.3"/>
      <circle cx="15" cy="15" r="2" fill="#FFFFFF" opacity="0.3"/>
      <circle cx="25" cy="15" r="2" fill="#FFFFFF" opacity="0.3"/>
      <circle cx="15" cy="25" r="2" fill="#FFFFFF" opacity="0.3"/>
      <circle cx="25" cy="25" r="2" fill="#FFFFFF" opacity="0.3"/>
    </pattern>
  </defs>
  
  <!-- 卡片背景 -->
  <rect width="200" height="200" fill="url(#cardGradient)" rx="15"/>
  
  <!-- 边框 -->
  <rect x="10" y="10" width="180" height="180" fill="none" stroke="#FFFFFF" stroke-width="3" rx="10"/>
  
  <!-- 爪印图案 -->
  <rect x="10" y="10" width="180" height="180" fill="url(#pawPattern)" rx="10"/>
  
  <!-- 中央问号 -->
  <circle cx="100" cy="100" r="40" fill="#FFFFFF" opacity="0.9"/>
  <text x="100" y="120" text-anchor="middle" fill="#2E7D32" font-family="Arial, sans-serif" font-size="48" font-weight="bold">?</text>
  
  <!-- 装饰性边框 -->
  <rect x="5" y="5" width="190" height="190" fill="none" stroke="#FFFFFF" stroke-width="2" rx="15" opacity="0.7"/>
</svg>